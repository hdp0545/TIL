body {
	font-size: 11pt;
	line-height: 150%;
	margin: 0;
}

.html1 {
	width: 700px;
	margin: 20px;
	border: 1px solid gray;
}

.html1 > .title{
	background-color: #c7c7c7;
	border-bottom: 1px solid gray;
	padding: 10px;
}

.html1 > .title > h1 {
	display: inline;
}

.html1 > .title > a {
	padding: 0 0 0 30px;
	text-decoration: none;
	color: red;
}

.html1 > .data {
	padding: 10px;
}

.html1 > .data > img {
	padding: 10px;
	vertical-align: top;
	border: 1px solid #575757;
	-moz-box-shadow: 2px 2px 5px #787878;
	-webkit-box-shadow: 2px 2px 5px #787878;
	box-shadow: 2px 2px 5px #787878;
}

.html1 > .data > table {
	padding: 0;
	vertical-align: top;
	line-height: 100%;
	display: inline-block;
	font-size: 10pt;
}

.html1 > .data > table th {
	text-align: right;
	vertical-align: top;
	padding-right: 20px;
	padding-left: 40px;
}

.html1 > .desc {
	margin: 0;
	padding: 10px;
}

.html1 > .desc p {
	margin: 0 0 15px;
	padding: 0;
}

.copyright, .copyright a {
	width: 700px;
	text-align: right;
	padding: 0 10px 10px;
	font-size: 10pt;
	text-decoration: none;
	color: #183ae7;
}
#tipBox {
	position: absolute;
	display: none;
	font-size: 0.7em;
	padding: 5px;
	border: 1px solid #ccc;
	background-color: yellow;
}

.toolTip {
	border-bottom: 1px dotted red;
}